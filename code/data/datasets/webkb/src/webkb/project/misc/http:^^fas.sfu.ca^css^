<script language="javascript">
<!-- Hide from Non-Netscape People
var annWin = null;
function openAnn() {
      annWin = window.open("", "announce", 'width=200,height=400,resizable=1');
        if (annWin != null) {
            annWin.rootWin = self;
            annWin.location = "http://fas.sfu.ca/css/ann.cgi";
    }

}
function stat(txt) {
  window.status = txt;
  setTimeout("erase()", 3000);
}

function erase() {
  window.status="";
}


// -->
