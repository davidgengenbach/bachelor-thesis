<SCRIPT LANGUAGE="JavaScript">
<!-- Start JavaScript --
function scrollmsg(msgpos)
{
  var sleep=125;
  var blanks="               ";
  var msg1="Hello!      ";
  var msg2="for Internet Wizards, Visionaries, Builders and Doers.";
  var msg3="Check us out at CRPC!";
  var msg=blanks+blanks+blanks+blanks+blanks+msg1+blanks+msg2+blanks+msg3;
  out=msg.substring(msgpos,msg.length);
  window.status=out;
  msgpos++;
  if (msgpos <= msg.length) {
    var exec="scrollmsg("+msgpos+")";
    zzz=window.setTimeout(exec,sleep);
  }
}
// -- End JavaScript -->
</SCRIPT>

<BODY BGCOLOR="#ffffff" onLoad="zzz=window.setTimeout('scrollmsg(1)',1);">

