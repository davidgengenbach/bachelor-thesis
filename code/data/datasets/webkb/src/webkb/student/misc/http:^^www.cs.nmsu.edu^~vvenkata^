var id, pause=0, position=0;
function banner(){
  //variables declaration
  var i,k,msg=" Welcome to the Vijay Arun's Home at NMSU! "
  //increase msg
  k=(50/msg.length)+1;
  for(i=0;i<=k;i++) msg+=" "+msg;
