    CLASSID="clsid:A23D7C20-CABA-11CF-A5D4-00AA00A47DD2"
        CODEBASE="http://www.eecs.tulane.edu/www/Ellington.Bryan/iepopwnd.ocx#Version=4,70,0,1161"
