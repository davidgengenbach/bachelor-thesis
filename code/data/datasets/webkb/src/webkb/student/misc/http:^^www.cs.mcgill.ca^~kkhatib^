<!-- Beginning of JavaScript Applet -------------------

/* Provided By Web Integration Systems, Inc. DBA Websys, Inc. All Rights Reserved.*/

function scrollit_r2l(seed)
{
        var m1 = "heay.. Don't forget ";
        var m2 = " to see the pictures!!!";
        var m3 = " ";
        var m4 = " ";

        var m6 = "";

        var msg= m1 + m2 + m3 + m4 +m6;
        var out = " ";
        var c = 1;

        if (seed > 100) {
                seed--;
                var cmd="scrollit_r2l(" + seed + ")";
                timerTwo=window.setTimeout(cmd,50);
        }
        else if (seed <= 100 && seed > 0) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="scrollit_r2l(" + seed + ")";
                window.status=out;
                timerTwo=window.setTimeout(cmd,50);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="scrollit_r2l(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,50);
                }

                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("scrollit_r2l(100)",45);
                }
        }
}


// -- End of JavaScript code -------------- -->



</script>




